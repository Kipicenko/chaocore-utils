{
  "name": "@yachao/utils",
  "version": "1.0.0",
  "private": false,
  "sideEffects": false,
  "description": "utils",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:lib": "rollup -c --bundleConfigAsCjs",
    "build:clean": "rimraf dist",
    "test": "jest",
    "tscheck": "yarn tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "yarn tscheck && yarn eslint",
    "src/**/__tests__/*.test.ts": "yarn test"
  },
  "homepage": "https://github.com/Kipicenko/yachao-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kipicenko/yachao-utils"
  },
  "author": "Kipicenko",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "rollup": "^4.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}

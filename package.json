{
  "name": "@chaocore/utils",
  "version": "0.0.0",
  "private": false,
  "sideEffects": false,
  "description": "My utils library for JavaScript and TypeScript.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn build:lib && yarn build:types",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c --bundleConfigAsCjs",
    "build:types": "yarn tsc --project tsconfig.declaration.json",
    "all:checks": "yarn tscheck && yarn eslint && yarn test",
    "tscheck": "yarn tsc --noEmit",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/!(__tests__)/*.ts": ["yarn tsc-files --noEmit", "yarn eslint"],
    "src/**/__tests__/*.test.ts": ["yarn eslint", "yarn test"]
  },
  "homepage": "https://github.com/Kipicenko/chaocore-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kipicenko/chaocore-utils"
  },
  "bugs": {
    "url": "https://github.com/Kipicenko/chaocore-utils/issues"
  },
  "keywords": [
    "utils",
    "utils-library",
    "helpers",
    "helpers-library",
    "javascript",
    "typescript",
    "programming"
  ],
  "author": "Kipicenko",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "rollup": "^4.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.4",
    "typescript": "^5.2.2"
  }
}
